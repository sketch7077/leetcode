class Solution {
    public int numJewelsInStones(String jewels, String stones) {
            int count=0;
            for(int i=0;i<jewels.length();i++)
            {
                char x=jewels.charAt(i);
                for(int j=0;j<stones.length();j++)
                {
                    char y=stones.charAt(j);
                    if((int)x==(int)y)
                        count++;
                }
            }
            return count;    
    }
}
